{
  "api": "/api/order/create",
  "method": "post",
  "inputType": "application/json",
  "input": {
    "customerId": "integer (شناسه مشتری، الزامی)",
    "restaurantId": "integer (شناسه رستوران، الزامی)",
    "items": [
      {
        "foodId": "integer (شناسه غذا، الزامی)",
        "quantity": "integer (تعداد، پیش‌فرض 1)"
      }
    ]
  },
  "output": [
    {
      "status": "success",
      "message": "سفارش با موفقیت ثبت شد.",
      "orderId": "integer (شناسه سفارش)",
      "statusCode": 201
    },
    {
      "status": "error",
      "message": "اطلاعات سفارش ناقص است.",
      "statusCode": 400
    }
  ]
}
